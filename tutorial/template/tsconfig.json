{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": true,
        "allowJs": true,
        "strict": true,
        "module": "ES6",
        "target": "es5",
        "jsx": "react",
        "allowSyntheticDefaultImports": true // import React from 'react'でerrorとなったため追加
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ],
}